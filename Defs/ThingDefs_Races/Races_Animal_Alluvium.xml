<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="AnimalThingBase">
    <defName>Bast_Alluvium</defName>
    <label>alluvium</label>
    <description>As a near weightless clump of semi-solidified pink and purple goo, Alluvium are similar to ancient Earth-dwelling amphibians. They move slow and have practically no armor making them an easy target for humans and beasts alike.\n\nInside their gelatinous bodies you will find a soft, sticky-sweet jelly with a taste to match it's delicious hue!</description>
    <statBases>
      <MoveSpeed>2.25</MoveSpeed>
      <ComfyTemperatureMin>-25</ComfyTemperatureMin>
      <MarketValue>150</MarketValue>
      <LeatherAmount>0</LeatherAmount>
      <MeatAmount>0</MeatAmount>
      <ArmorRating_Blunt>0.10</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.10</ArmorRating_Sharp>
    </statBases>
    <comps>
      <li Class="AnimalBehaviours.CompProperties_AsexualReproduction">
        <reproductionIntervalDays>8</reproductionIntervalDays>
        <customString>Duplicating in: </customString>
      </li>
      <li Class="AnimalBehaviours.CompProperties_AnimalProduct">
				<resourceDef>Bast_Alluvium_Jelly</resourceDef>
				<gatheringIntervalDays>2</gatheringIntervalDays>
				<resourceAmount>20</resourceAmount>
        <customResourceString>alluvium jelly</customResourceString>
        <isRandom>false</isRandom>
			</li>
    </comps>
    <tools>
			<li>
				<label>Bast_Nucleoplasm</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>0.01</power>
				<cooldownTime>10</cooldownTime>
				<linkedBodyPartsGroup>Bast_Body</linkedBodyPartsGroup>
				<ensureLinkedBodyPartsGroupAlwaysUsable>false</ensureLinkedBodyPartsGroupAlwaysUsable>
				<chanceFactor>0.01</chanceFactor>
			</li>
		</tools>
    <race>
      <herdAnimal>true</herdAnimal>
      <body>Bast_LimblessGelatinous</body>
      <baseHungerRate>0.05</baseHungerRate>
      <foodType>OmnivoreAnimal</foodType>
      <baseBodySize>1.0</baseBodySize>
      <baseHealthScale>1.7</baseHealthScale>
      <wildness>0.03</wildness>
      <trainability>None</trainability>
      <manhunterOnTameFailChance>0.00</manhunterOnTameFailChance>
			<manhunterOnDamageChance>0.00</manhunterOnDamageChance>
      <canBePredatorPrey>true</canBePredatorPrey>
      <lifeExpectancy>9</lifeExpectancy>
      <hasGenders>false</hasGenders>
      <mateMtbHours>1000</mateMtbHours>
			<gestationPeriodDays>1000</gestationPeriodDays>
      <wildBiomes>
        <IceSheet>0.00</IceSheet>
				<SeaIce>0.00</SeaIce>
        <ColdBog>0.02</ColdBog>
        <Desert>0.20</Desert>
        <BorealForest>0.15</BorealForest>
        <Tundra>0.05</Tundra>
				<TemperateForest>0.25</TemperateForest>
				<TemperateSwamp>0.20</TemperateSwamp>
				<TropicalRainforest>0.15</TropicalRainforest>
				<TropicalSwamp>0.20</TropicalSwamp>
				<AridShrubland>0.20</AridShrubland>
			</wildBiomes>
      <lifeStageAges>
        <li>
          <def>AnimalBaby</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>AnimalJuvenile</def>
          <minAge>.25</minAge>
        </li>
        <li>
          <def>AnimalAdult</def>
          <minAge>.50</minAge>
          <soundWounded>Pawn_Rodent_Wounded</soundWounded>
          <soundDeath>Pawn_Rodent_Wounded</soundDeath>
          <soundCall>Pawn_Rodent_Call</soundCall>
          <soundAngry>Pawn_Rodent_Angry</soundAngry>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
    </race>
    <butcherProducts>
			  <Bast_AlluviumFlesh>10</Bast_AlluviumFlesh>
		</butcherProducts>
    <tradeTags>
      <li>AnimalUncommon</li>
    </tradeTags>
  </ThingDef>

  <PawnKindDef ParentName="AnimalKindBase">
    <defName>Bast_Alluvium</defName>
    <label>alluvium</label>
    <race>Bast_Alluvium</race>
    <combatPower>10</combatPower>
    <canArriveManhunter>false</canArriveManhunter>
    <ecoSystemWeight>0.03</ecoSystemWeight>
    <lifeStages>
      <li>
        <bodyGraphicData>
          <texPath>Things/Pawn/Animal/Bast_Alluvium/Bast_Alluvium</texPath>
          <drawSize>1</drawSize>
        </bodyGraphicData>
        <dessicatedBodyGraphicData>
          <texPath>Things/Pawn/Animal/Bast_Alluvium/Bast_Dessicated_Alluvium</texPath>
          <drawSize>1</drawSize>
        </dessicatedBodyGraphicData>
      </li>
      <li>
        <bodyGraphicData>
          <texPath>Things/Pawn/Animal/Bast_Alluvium/Bast_Alluvium</texPath>
          <drawSize>1.55</drawSize>
        </bodyGraphicData>
        <dessicatedBodyGraphicData>
          <texPath>Things/Pawn/Animal/Bast_Alluvium/Bast_Dessicated_Alluvium</texPath>
          <drawSize>1.55</drawSize>
        </dessicatedBodyGraphicData>
      </li>
      <li>
        <bodyGraphicData>
          <texPath>Things/Pawn/Animal/Bast_Alluvium/Bast_Alluvium</texPath>
          <drawSize>2.33</drawSize>
        </bodyGraphicData>
        <dessicatedBodyGraphicData>
          <texPath>Things/Pawn/Animal/Bast_Alluvium/Bast_Dessicated_Alluvium</texPath>
          <drawSize>2.33</drawSize>
        </dessicatedBodyGraphicData>
      </li>
    </lifeStages>
  </PawnKindDef>
  
</Defs>