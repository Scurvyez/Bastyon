<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <!--==================== Alluvium Pawn Sounds ====================-->

    <SoundDef>
        <defName>Pawn_Alluvium_Call</defName>
        <context>MapOnly</context>
        <subSounds>
            <li>
                <grains>
                    <li Class="AudioGrain_Folder">
                        <clipFolderPath>Pawn/Animal/Alluvium/Alluvium_Call</clipFolderPath>
                    </li>
                </grains>
                <volumeRange>15.42672~14.7734</volumeRange>
                <pitchRange>0.9770588~1.137647</pitchRange>
                <distRange>0~50.40025</distRange>
                <sustainLoop>False</sustainLoop>
            </li>
        </subSounds>
    </SoundDef>

