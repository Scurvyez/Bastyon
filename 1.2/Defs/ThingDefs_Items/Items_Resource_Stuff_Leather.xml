<Defs>

    <!--==================== Bahlrin Leather ====================-->

    <ThingDef ParentName="LeatherBase">
        <defName>Leather_Bahlrin</defName>
        <label>bahlrin leather</label>
        <description>A Bahlrin's hide is not beautiful. It is not good at insulating. However, the heavy dark-grayish skin is astonishingly durable. Stopping most sharp attcks in their tracks, this alien leather is perfect for the warrior pawns of your colony!</description>
        <graphicData>
            <color>(69,69,69)</color>
        </graphicData>
        <statBases>
            <MarketValue>2.0</MarketValue>
            <StuffPower_Armor_Sharp>.82</StuffPower_Armor_Sharp>
            <StuffPower_Armor_Blunt>.02</StuffPower_Armor_Blunt>
            <StuffPower_Armor_Heat>.12</StuffPower_Armor_Heat>
            <StuffPower_Insulation_Cold>-10</StuffPower_Insulation_Cold>
            <DeteriorationRate>1.25</DeteriorationRate>
            <Flammability>.25</Flammability>
            <MaxHitPoints>340</MaxHitPoints>
            <Mass>0.018</Mass>
        </statBases>
        <burnableByRecipe>true</burnableByRecipe>
        <healthAffectsPrice>true</healthAffectsPrice>
        <minRewardCount>35</minRewardCount>
        <stuffProps>
            <statFactors>
                <Beauty>0.4</Beauty>
            </statFactors>
            <categories>
                <li>Leathery</li>
            </categories>
            <color>(69,69,69)</color>
            <commonality>0.025</commonality>
        </stuffProps>
    </ThingDef>

