<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <!--==================== Ant Body ====================-->

    <BodyDef>
		<defName>Bast_Ant</defName>
		<label>ant body</label>
		<corePart>
			<def>Body</def>
			<height>Middle</height>
			<depth>Outside</depth>
			<parts>
				<li>
					<def>Bast_Thorax</def>
					<coverage>0.10</coverage>
				</li>
				<li>
					<def>Stomach</def>
					<coverage>0.05</coverage>
					<depth>Inside</depth>
				</li>
				<li>
					<def>InsectHeart</def>
					<coverage>0.03</coverage>
					<depth>Inside</depth>
				</li>
				<li>
					<def>Pronotum</def>
					<coverage>0.08</coverage>
					<height>Top</height>
					<parts>
						<li>
							<def>InsectHead</def>
							<coverage>0.65</coverage>
							<groups>
								<li>HeadAttackTool</li>
							</groups>
							<parts>
								<li>
									<def>Brain</def>
									<coverage>0.15</coverage>
									<depth>Inside</depth>
								</li>
								<li>
									<def>Eye</def>
									<customLabel>left first compound eye</customLabel>
									<coverage>0.02</coverage>
								</li>
								<li>
									<def>Eye</def>
									<customLabel>left second compound eye</customLabel>
									<coverage>0.02</coverage>
								</li>
								<li>
									<def>Eye</def>
									<customLabel>right first compound eye</customLabel>
									<coverage>0.02</coverage>
								</li>
                                <li>
									<def>Eye</def>
									<customLabel>right second compound eye</customLabel>
									<coverage>0.02</coverage>
								</li>
								<li>
									<def>InsectNostril</def>
									<coverage>0.1</coverage>
								</li>
								<li>
									<def>Bast_InsectMouth</def>
									<coverage>0.05</coverage>
									<groups>
										<li>Mouth</li>
									</groups>
								</li>
                                <li>
									<def>Bast_Antennae</def>
									<customLabel>right antennae</customLabel>
									<coverage>0.01</coverage>
								</li>
                                <li>
									<def>Bast_Antennae</def>
									<customLabel>right antennae</customLabel>
									<coverage>0.01</coverage>
								</li>
							</parts>
						</li>
					</parts>
				</li>
				<li>
					<def>Bast_Abdomen</def>
					<coverage>0.08</coverage>
					<height>Bottom</height>
				</li>
				<li>
					<def>Bast_SmallInsectLeg</def>
					<customLabel>left first leg</customLabel>
					<coverage>0.02</coverage>
					<height>Middle</height>
					<groups>
						<li>FrontLeftLeg</li>
					</groups>
				</li>
				<li>
					<def>Bast_SmallInsectLeg</def>
					<customLabel>right first leg</customLabel>
					<coverage>0.02</coverage>
					<height>Middle</height>
					<groups>
						<li>FrontRightLeg</li>
					</groups>
				</li>
				<li>
					<def>Bast_SmallInsectLeg</def>
					<customLabel>left second leg</customLabel>
					<coverage>0.02</coverage>
					<height>Middle</height>
				</li>
				<li>
					<def>Bast_SmallInsectLeg</def>
					<customLabel>right second leg</customLabel>
					<coverage>0.02</coverage>
					<height>Middle</height>
				</li>
                <li>
					<def>Bast_SmallInsectLeg</def>
					<customLabel>left third leg</customLabel>
					<coverage>0.02</coverage>
					<height>Middle</height>
				</li>
				<li>
					<def>Bast_SmallInsectLeg</def>
					<customLabel>right third leg</customLabel>
					<coverage>0.02</coverage>
					<height>Middle</height>
				</li>
				
			</parts>
		</corePart>
	</BodyDef>
	
</Defs>